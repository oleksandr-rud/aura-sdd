# Development Environment Variables
# Copy this file to .env and fill in your values

# Image Registry
IMAGE_REGISTRY=your-registry.com
VERSION=latest

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/specgen_dev

# Authentication
JWT_SECRET=your-jwt-secret-here

# Redis (optional)
REDIS_URL=redis://localhost:6379

# Ingress
INGRESS_HOST=dev.spec-gen.local

# Load Balancer (if using external LB)
LOAD_BALANCER_IP=

# API Configuration
API_PORT=4000
API_HOST=0.0.0.0

# Client Configuration
VITE_API_BASE_URL=http://api:4000