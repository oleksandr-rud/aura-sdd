{
  "name": "spec-gen-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "concurrently \"pnpm --filter @chat-app/app dev\" \"pnpm --filter @chat-app/api dev\"",
    "dev:full": "./dev-startup.sh start",
    "dev:stop": "./dev-startup.sh stop",
    "dev:restart": "./dev-startup.sh restart",
    "dev:status": "./dev-startup.sh status",
    "dev:install": "./dev-startup.sh install",
    "dev:help": "./dev-startup.sh help",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
